.goods-wrap{
    display: flex;
    margin-bottom: 20px;
    margin-right: 15px;
  }
  .goods-img-wrap{
    display: flex;
    width: 110px;
    height: 60px;
    border-radius: 10px;
    margin-right: 10px;
  }
  .goods-img{
    width: 100%;
    height: 100%;
    border-radius: 5px;
  }
  .goods-info{
    flex: 1;
  }
  .goods-title{
    display: flex;
    font-size: 14px;
    color: #333333;
    
  }
  
  .goods-info image {
    
    width: 22px;
    height: 22px;
  }
  .goods-others {
    margin-top: 18px;
    display: flex;
    color: #767171;
    font-size: 13px;
  }
  .goods-score image {
    width: 18px;
    height: 15px;
  }
  .goods-sale {
    margin-left: 25px;
  }
  .shopcar image {
    width: 25px;
    height: 25px;
  }