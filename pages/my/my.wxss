    margin-top: 0px;
}
.userinfoblock {
    display: flex;
}
.userinfotext {
    margin-top: 15px;
}
.userinfotext .name {
    font-weight: 500;
    font-size: 20px;
    color: #343434;
}
.userinfotext .sign {
    margin-top: 15px;
    font-size: 13px;
    color: #969696;
}

.userinfoimg image {
    margin-left: 45px;
    width: 80px;
    height: 80px;
}
